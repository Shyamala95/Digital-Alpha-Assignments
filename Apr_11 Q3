#multiply 
temp = str(n)
t1 = temp+temp
t2 = temp + temp + temp
comp = n+int(t1)+int(t2)
print("value of n+nn+nnn:", comp)

for n in range(1,n+1):
    array.append(1)
    for i in range(n-2 , 0,-1):
        array[i] += array[i-1]
    print("ROW",n,array)
    
    

